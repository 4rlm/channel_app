<!-- # Duplicate info from new.erb and edit.erb forms goes in this partial. -->

<div class="form-group">
  <label for="name" class="col-sm-2 control-label">Name</label>
  <div class="col-sm-10">
    <input class="form-control" id="name" name="channel[name]" type="text" value="<%= @channel.name %>" placeholder="Name" />
  </div>
</div>

<div class="form-group">
  <label for="callsign" class="col-sm-2 control-label">Callsign</label>
  <div class="col-sm-10">
    <input class="form-control" id="callsign" name="channel[callsign]" type="text" value="<%= @channel.callsign %>" placeholder="Callsign" />
  </div>
</div>

<div class="form-group">
  <label for="price_per_month" class="col-sm-2 control-label">Price Per Month</label>
  <div class="col-sm-10">
    <input class="form-control" id="price_per_month" name="channel[price_per_month]" type="text" value="<%= @channel.price_per_month %>" placeholder="Price Per Month" />
  </div>
</div>



